{
  "name": "logger-neue",
  "version": "0.6.0",
  "description": "Doing for Node logging what Neue did for Helvetica.",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "babel src -d dist",
    "watch": "babel src -d dist -w",
    "test": "ava"
  },
  "keywords": [
    "logging",
    "node",
    "logger",
    "console",
    "colors",
    "chalk",
    "formatted"
  ],
  "author": "Bo Lingen <thecitycide@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "assign-deep": "^0.4.5",
    "chalk": "^1.1.3",
    "fs-promise": "^1.0.0",
    "strat": "^0.6.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "verbose": true,
    "babel": "inherit"
  },
  "babel": {
    "presets": [],
    "plugins": [
      "add-module-exports",
      "transform-es2015-modules-commonjs"
    ]
  }
}
